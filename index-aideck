        <!-- Navigation -->
        <div class="navigation">
            <button class="nav-btn" onclick="previousSlide()">
                <i class="fas fa-chevron-left"></i>
            </button>
            <button class="nav-btn" onclick="nextSlide()">
                <i class="fas fa-chevron-right"></i>
            </button>
        </div>

        <!-- Slide Counter -->
        <div class="slide-counter">
            <span id="currentSlide">1</span> / <span id="totalSlides">16</span>
        </div>
    </div>

    <script>
        let currentSlideIndex = 0;
        const slides = document.querySelectorAll('.slide');
        const totalSlides = slides.length;
        let timerInterval;
        let initialConfidence = null;
        let finalConfidence = null;
        let votes = { sales: 0, gtm: 0, marketing: 0 };
        let selectedMethod = 'care';

        // Initialize
        document.getElementById('totalSlides').textContent = totalSlides;
        updateProgressBar();

        function showSlide(index) {
            slides.forEach(slide => slide.classList.remove('active'));
            slides[index].classList.add('active');
            document.getElementById('currentSlide').textContent = index + 1;
            updateProgressBar();
        }

        function nextSlide() {
            if (currentSlideIndex < totalSlides - 1) {
                currentSlideIndex++;
                showSlide(currentSlideIndex);
            }
        }

        function previousSlide() {
            if (currentSlideIndex > 0) {
                currentSlideIndex--;
                showSlide(currentSlideIndex);
            }
        }

        function updateProgressBar() {
            const progress = ((currentSlideIndex + 1) / totalSlides) * 100;
            document.getElementById('progressBar').style.width = progress + '%';
        }

        function selectMethod(method) {
            selectedMethod = method;
            
            // Reset all cards
            document.getElementById('careCard').classList.remove('selected');
            document.getElementById('chainCard').classList.remove('selected');
            document.getElementById('personaCard').classList.remove('selected');
            
            // Reset all indicators
            document.getElementById('careIndicator').innerHTML = '◯ Alternative';
            document.getElementById('careIndicator').style.color = '#f97316';
            document.getElementById('chainIndicator').innerHTML = '◯ Alternative';
            document.getElementById('chainIndicator').style.color = '#f97316';
            document.getElementById('personaIndicator').innerHTML = '◯ Alternative';
            document.getElementById('personaIndicator').style.color = '#f97316';
            
            // Set selected method
            const cards = {
                care: 'careCard',
                chain: 'chainCard', 
                persona: 'personaCard'
            };
            
            const indicators = {
                care: 'careIndicator',
                chain: 'chainIndicator', 
                persona: 'personaIndicator'
            };
            
            document.getElementById(cards[method]).classList.add('selected');
            document.getElementById(indicators[method]).innerHTML = '✓ Selected Focus';
            document.getElementById(indicators[method]).style.color = '#22c55e';
        }

        function selectConfidence(score) {
            initialConfidence = score;
            document.getElementById('selectedConfidence').textContent = `Selected: ${score}`;
            document.getElementById('beforeScore').textContent = score;
            
            // Highlight selected number
            const scaleNumbers = document.querySelectorAll('.confidence-scale .scale-number');
            scaleNumbers.forEach(num => num.classList.remove('selected'));
            event.target.classList.add('selected');
        }

        function selectNewConfidence(score) {
            finalConfidence = score;
            document.getElementById('afterScore').textContent = score;
            
            if (initialConfidence !== null) {
                const improvement = finalConfidence - initialConfidence;
                document.getElementById('improvement').textContent = improvement >= 0 ? `+${improvement}` : improvement;
            }
            
            // Highlight selected number
            const scaleNumbers = document.querySelectorAll('.confidence-scale .scale-number');
            scaleNumbers.forEach(num => num.classList.remove('selected'));
            event.target.classList.add('selected');
        }

        function toggleCheck(checkbox) {
            checkbox.classList.toggle('checked');
            if (checkbox.classList.contains('checked')) {
                checkbox.innerHTML = '✓';
            } else {
                checkbox.innerHTML = '';
            }
        }

        function startTimer(seconds) {
            clearInterval(timerInterval);
            let timeLeft = seconds;
            
            const timerElements = document.querySelectorAll('.timer');
            
            timerInterval = setInterval(() => {
                const minutes = Math.floor(timeLeft / 60);
                const secs = timeLeft % 60;
                const timeString = `${minutes}:${secs.toString().padStart(2, '0')}`;
                
                timerElements.forEach(timer => {
                    if (timer.offsetParent !== null) { // Check if timer is visible
                        timer.textContent = timeString;
                        
                        // Add urgency styling when time is low
                        if (timeLeft <= 60) {
                            timer.style.color = '#ef4444';
                            timer.style.animation = 'pulse 1s infinite';
                        } else if (timeLeft <= 180) {
                            timer.style.color = '#f97316';
                        }
                    }
                });
                
                if (timeLeft <= 0) {
                    clearInterval(timerInterval);
                    timerElements.forEach(timer => {
                        if (timer.offsetParent !== null) {
                            timer.textContent = "TIME'S UP!";
                            timer.style.color = '#ef4444';
                        }
                    });
                }
                
                timeLeft--;
            }, 1000);
        }

        function voteModule(module) {
            votes[module]++;
            document.getElementById(module + 'Votes').textContent = votes[module] + ' votes';
            
            // Add visual feedback
            event.target.closest('.benefit-card').style.transform = 'scale(1.05)';
            setTimeout(() => {
                event.target.closest('.benefit-card').style.transform = 'scale(1)';
            }, 200);
        }

        function showWinner() {
            // Find the module with most votes
            let winner = 'sales';
            let maxVotes = votes.sales;
            
            if (votes.gtm > maxVotes) {
                winner = 'gtm';
                maxVotes = votes.gtm;
            }
            if (votes.marketing > maxVotes) {
                winner = 'marketing';
                maxVotes = votes.marketing;
            }
            
            // Update the bonus module slide based on winner
            const titles = {
                sales: 'Sales Strategy',
                gtm: 'GTM Timeline', 
                marketing: 'Marketing Campaign'
            };
            
            const methods = {
                sales: 'Persona Method',
                gtm: 'Chain Method',
                marketing: 'C-A-R-E Method'
            };
            
            const descriptions = {
                sales: 'Building your complete sales strategy using Persona Method',
                gtm: 'Creating your 90-day launch timeline using Chain Method',
                marketing: 'Developing your content calendar using C-A-R-E Method'
            };
            
            const prompts = {
                sales: `<strong>🎯 SALES STRATEGY PROMPT (Persona Method):</strong><br><br>
                        <strong>Persona:</strong> You are David Rodriguez, senior sales strategist with 12+ years helping premium construction companies scale revenue. MBA from Rice University, previously at Deloitte, specialized in high-value B2B sales ($75K+ average deals). You understand construction sales cycles, multiple stakeholders (homeowner, spouse, architect, sometimes HOA), seasonal patterns, and relationship-based selling in affluent markets.<br><br>
                        <strong>Context:</strong> Buda Builders sustainable construction company needs systematic sales growth. Current: 20 leads/month (mostly referrals), 45% close rate, $145K average project value, 90-day average sales cycle. Goal: 40 leads/month, 60% close rate, maintain pricing premium. Team: founder + 1 sales coordinator, $8K/month marketing budget.<br><br>
                        <strong>Task:</strong> Design complete sales system including: lead qualification framework (BANT + custom criteria), 6-touch nurture sequence with templates, presentation framework for in-home consultations, objection handling guide for top 8 concerns, proposal template with pricing psychology, follow-up automation, referral program structure, and CRM workflow. Focus on sustainable construction expertise as key differentiator.<br><br>
                        <strong>Result:</strong> Scalable sales process that doubles qualified opportunities within 90 days while maintaining premium positioning and high close rates.`,
                        
                gtm: `<strong>🚀 GTM STRATEGY PROMPT (Chain Method):</strong><br><br>
                      <strong>Input:</strong> My complete business plan will be pasted between <<<BP_START>>> and <<<BP_END>>>. Use it to ground readiness and projections.<br><br><strong>Step 1:</strong> "Analyze go-to-market readiness for Buda Builders expanding into premium eco-luxury market segment. Current position: established local reputation, $1.8M revenue, 50+ completed projects. New target: $250K+ custom eco-homes for affluent sustainability-focused buyers. Assess: market size and competition analysis, operational capacity gaps, brand positioning requirements, pricing strategy optimization, and identify 4 critical success factors plus 3 major risks for premium market entry."<br><br>
                      <strong>Step 2:</strong> "Create detailed 120-day market entry timeline based on readiness analysis. Include: weeks 1-30 (brand elevation, portfolio development), weeks 31-60 (marketing launch, strategic partnerships), weeks 61-90 (sales execution, lead generation), weeks 91-120 (optimization, scaling). Assign specific deliverables, budget allocation ($25K total), team responsibilities, and weekly milestones with measurable outcomes."<br><br>
                      <strong>Step 3:</strong> "Develop comprehensive success metrics and risk mitigation plan. Create: performance dashboard tracking 12 key metrics (lead quality, conversion rates, project margins), contingency plans for each identified risk, monthly review checkpoints, pivot strategies if targets missed, and 6-month post-launch growth projections with expansion planning. Design: Provide a Gantt-style 16:9 timeline with weekly swimlanes (Brand, Marketing, Sales, Ops), milestone icons, and color‑coded risk flags. Include slide notes for presentation."`,
                      
                marketing: `<strong>📱 MARKETING CAMPAIGN PROMPT (C-A-R-E Method):</strong><br><br>
                          <strong>Input:</strong> My complete business plan will be pasted between <<<BP_START>>> and <<<BP_END>>>. Base segments, creative angles, and budgets on it.<br><br><strong>Context:</strong> Buda Builders needs systematic lead generation beyond referrals. Target: affluent Central Texas homeowners (income $200K+, age 35-60, value sustainability). Current: 500 Instagram followers, 150 past clients, strong Google reviews (4.9/5). Budget: $12K for 60-day campaign. Goal: establish predictable lead flow for premium projects ($150K+ average).<br><br>
                          <strong>Action:</strong> Create integrated marketing campaign including: Google Ads strategy (search + display), social media content system (Instagram + Facebook), email marketing automation, local partnership development, and content marketing plan. All materials should emphasize sustainability expertise, showcase premium craftsmanship, and target eco-conscious luxury market.<br><br>
                          <strong>Result:</strong> Generate 35 qualified leads (verified budget $150K+, timeline 6-18 months) and establish Buda Builders as the premium sustainable builder in Central Texas within 60 days.<br><br>
                          <strong>Example:</strong> Weekly content calendar with specific post themes, Google Ads campaigns with keyword lists and ad copy variations, email sequences for different buyer stages, partnership proposal templates for eco-luxury brands, tracking dashboard with conversion metrics, and creative guidelines using earth tones (#2E7D32 primary, #8D6E63 secondary, #FFF8E1 accent) with premium photography standards.`
            };
            
            document.getElementById('winnerTitle').textContent = titles[winner];
            document.getElementById('methodUsed').textContent = methods[winner];
            document.getElementById('bonusDescription').textContent = descriptions[winner];
            document.getElementById('bonusPrompt').innerHTML = prompts[winner];
            document.getElementById('bonusBuilt').textContent = titles[winner];
            document.getElementById('methodBuilt').textContent = methods[winner];
            
            // Move to bonus module slide
            currentSlideIndex = 10; // Slide 11 (0-indexed)
            showSlide(currentSlideIndex);
        }

        // Keyboard navigation
        document.addEventListener('keydown', function(e) {
            if (e.key === 'ArrowRight' || e.key === ' ') {
                e.preventDefault();
                nextSlide();
            } else if (e.key === 'ArrowLeft') {
                e.preventDefault();
                previousSlide();
            }
        });

        // Initialize with first slide
        showSlide(0);
    </script>
</body>
</html>
